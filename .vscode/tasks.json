{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe 词法分析json文件生成器",
            "command": "D:\\mingw64\\bin\\g++.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-O3",
                "lexer_json.cpp",
                "token/*.cpp",
                "lexer/*.cpp",
                "json/*.cpp",
                "-o",
                "${fileDirname}\\lexer.exe",
                "-static",
                "-I",
                "${workspaceFolder}",
                "-std=c++17",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "调试器生成的任务。"
        },
